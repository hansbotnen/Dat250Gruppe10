<!DOCTYPE html>
<html lang="en">

<% include ../partials/head %>

<body>
    <% include ../partials/nav %>
    
    <% account.forEach(function(account) { %>
    <div class="card">
        <a href="accounts/<%= account.id%>">
            <img src="<%= account.photo %>" style="width:100%; height:50%">
            <div class="container">
            
                <h3><%= account.name %></h3>
                <p>Phone: <%= account.phone %></p>
                <p>Email: <%= account.email %></p>
            </div>
        </a>
    </div>
    <% }); %>
</body>

</html> 